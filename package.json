{
  "name": "believers-sword-app",
  "version": "1.3.2",
  "private": true,
  "author": "Jenuel Oras Ganawed <jenuelganawed936@gmail.com> (https://jenuelganawed.com)",
  "description": "Believers Sword App is a desktop application for learning and studying bible. This app is free to use.",
  "main": "background.js",
  "scripts": {
    "up:nightly:build": "yarn nightly:rename && cross-env DEVELOPMENT_MODE=false VUE_APP_NIGHTLY_BUILD=true VUE_APP_DEV=false vue-cli-service electron:build",
    "build": "yarn nightly:rename:reset && cross-env DEVELOPMENT_MODE=false VUE_APP_NIGHTLY_BUILD=false VUE_APP_DEV=false vue-cli-service electron:build",
    "serve": "yarn nightly:rename && cross-env DEVELOPMENT_MODE=true VUE_APP_NIGHTLY_BUILD=true VUE_APP_DEV=true vue-cli-service electron:serve",
    "serve:all": "concurrently \"yarn serve\" \"serve_be.bat\"",
    "serve:be": "serve_be.bat",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "release": "RELEASE=YES vue-cli-service electron:build",
    "up:local": "release_local.bat",
    "up:night": "nightly.bat",
    "up": "release.bat",
    "cert": "electron-builder create-self-signed-cert -p BroJenuel",
    "copy:dictionary": "copy-english-dictionary.bat",
    "nightly:rename": "json -I -f package.json -e \"this.name='believers-sword-app-dev-nightly'\"",
    "nightly:rename:reset": "json -I -f package.json -e \"this.name='believers-sword-app'\""
  },
  "dependencies": {
    "@intlify/vue-i18n-loader": "^4.2.0",
    "@supabase/supabase-js": "^1.35.6",
    "@tiptap/extension-blockquote": "^2.0.0-beta.15",
    "@tiptap/extension-code-block": "^2.0.0-beta.18",
    "@tiptap/extension-placeholder": "^2.0.0-beta.31",
    "@tiptap/extension-underline": "^2.0.0-beta.16",
    "@tiptap/starter-kit": "^2.0.0-beta.105",
    "@tiptap/vue-3": "^2.0.0-beta.59",
    "axios": "^0.27.2",
    "core-js": "^3.6.5",
    "dayjs": "^1.11.2",
    "electron-log": "^4.4.1",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "firebase": "^9.8.4",
    "knex": "^2.0.0",
    "pinia": "^2.0.14",
    "sanitize-html": "^2.7.1",
    "split.js": "^1.6.4",
    "sqlite3": "^5.0.2",
    "upath": "^2.0.1",
    "vue": "^3.2.31",
    "vue-i18n": "9",
    "vue-router": "^4.0.0-0",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.0.0-0",
    "youtube-vue3": "^0.1.15"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "concurrently": "^7.2.0",
    "cross-env": "^7.0.3",
    "electron": "^19.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^8.0.0",
    "naive-ui": "^2.16.7",
    "sass": "^1.37.5",
    "sass-loader": "^10",
    "typescript": "~4.1.5",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-windicss": "~1.0.2"
  }
}
